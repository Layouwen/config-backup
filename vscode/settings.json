{
  /* vscode config start */
  // 编辑器配置
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  // 防止缩进被覆盖（解决了html默认缩进4）
  "editor.detectIndentation": false,
  // 始终显示新tab展示文件内容
  "workbench.editor.enablePreview": false,
  // 控制台显示在右侧
  "workbench.sideBar.location": "right",
  // icon 主题
  "workbench.iconTheme": "material-icon-theme",
  // 设置主题以及图标
  "workbench.colorTheme": "Community Material Theme Darker",
  // git 配置
  "git.enabled": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.path": "D:\\cmder\\vendor\\git-for-windows\\bin\\git.exe",
  // 终端配置
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "terminal.explorerKind": "external",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "bash": {
      "path": "D:\\cmder\\vendor\\git-for-windows\\bin\\bash.exe",
      "icon": "terminal-bash"
    },
    "Ubuntu-20.04 (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": [
        "-d",
        "Ubuntu-20.04"
      ]
    }
  },
  // 声明不同后轴文件使用什么代码补全
  "files.associations": {
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript"
  },
  // 声明哪些文件可以使用 emmet
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  // 开启装饰器语法
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  // 设置对应文件的格式化插件
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // 定义 TypeScript 中类型断言后添加空格
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  /* vscode config end */
  /* vim start */
  // 配置 vim 使用系统剪贴板
  "editor.lineNumbers": "relative",
  "vim.hlsearch": true,
  "vim.searchHighlightColor": "#f8e71c",
  "vim.searchHighlightTextColor": "#333",
  "vim.searchMatchColor": "#f8e71c",
  "vim.searchMatchTextColor": "#333",
  "vim.useSystemClipboard": true,
  /* vim end */
  /* prettier start */
  // prettier配置
  "prettier.semi": false,
  "prettier.arrowParens": "avoid",
  "prettier.withNodeModules": true,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": false,
  "prettier.bracketSpacing": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.printWidth": 120,
  /* prettier end */
  /* Code Spell Checker start */
  // 不检查的英文字典
  "cSpell.userWords": [
    "autofetch",
    "classpath",
    "cmder",
    "dockercompose",
    "factorypath",
    "fileheader",
    "koro",
    "leetcode",
    "minapp",
    "todohighlight",
    "vetur",
    "vitepress",
    "vsintellicode",
    "webstorm",
    "wxml",
    "wxss"
  ],
  /* Code Spell Checker end */
  /* eslint start */
  // eslint配置
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown"
  ],
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  /* eslint end */
  /* koroFileHeader start */
  // 禁止创建文件注释 和 自动添加注释
  "fileheader.configObj": {
    "autoAdd": false,
    "createHeader": false
  },
  /* koroFileHeader end */
  /* minapp start */
  "minapp-vscode.disableAutoConfig": true,
  /* minapp end */
  /* markdown start */
  "markdown.preview.lineHeight": 2,
  /* markdown end */
  /* TODO Highlight start */
  // todohighlight 高亮显示
  "todohighlight.isEnable": true,
  /* TODO Highlight end */
  /* vetur start */
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  /* vetur end */
  /* GitLens start */
  "[ansible]": {
    "gitlens.codeLens.scopes": [
      "document"
    ]
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "diffEditor.ignoreTrimWhitespace": false,
  "code-runner.runInTerminal": true,
  "editor.inlineSuggest.enabled": true,
"editor.unicodeHighlight.nonBasicASCII": false,
  /* GitLens end */
  /* leetcode start */
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.workspaceFolder": "~/.leetcode",
  "leetcode.defaultLanguage": "javascript",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  /* leetcode end */
  /* fig start */
  // fig 自动补全的配置
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  /* fig end */
  // 需要同步或忽略的设置
  "settingsSync.ignoredSettings": [
    "leetcode.workspaceFolder",
    "-git.defaultCloneDirectory",
    "-git.path"
  ],
  // 是否同步键盘
  "settingsSync.keybindingsPerPlatform": false,
  // copilot 自动补全
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true,
    "typescript": true,
    "javascript": true
  },
  // 统一换行符
  "files.eol": "\n",
  // vim设置
  "vim.easymotion": true,
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // 取消高亮
    {
      "before": [
        "<leader>",
        "n",
        "s"
      ],
      "commands": [
        ":noh"
      ]
    },
    // 重命名
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    // 折叠代码
    {
      "before": [
        "<leader>",
        "["
      ],
      "commands": [
        {
          "command": "editor.fold"
        },
        {
          "command": "vim.remap",
          "args": {
            "after": [
              "$",
              "%"
            ]
          }
        }
      ]
    },
    // 回到开头
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    // 回到结尾
    {
      "before": [
        "L"
      ],
      "after": [
        "g",
        "_"
      ]
    },
    // 格式化
    {
      "before": [
        "<Leader>",
        "f",
        "d"
      ],
      "commands": [
        "editor.action.formatDocument"
      ],
    },
    // 合并下一行
    {
      "before": [
        "<Leader>",
        "j",
      ],
      "commands": [
        "editor.action.joinLines"
      ],
    },
    // 向下跳 5 行
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    // 向上跳 5 行
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    // 向下跳 5 行
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    // 向上跳 5 行
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    }
  ],
  "bracket-pair-colorizer-2.depreciation-notice": false,
  /* oneDarkPro start */
  // 选择不同主题
  // "oneDarkPro.editorTheme": "oneDarkPro",
  /* oneDarkPro end */
  /* background start */
  // 背景设置
  // "background.useDefault": false,
  // "background.style": {
  //   "content": "''",
  //   "pointer-events": "none",
  //   "position": "absolute",
  //   "z-index": "99999",
  //   "width": "100%",
  //   "height": "100%",
  //   "background-position": "0% 0%",
  //   "background-size": "cover",
  //   "background-repeat": "no-repeat",
  //   "opacity": 0.1
  // },
  // "background.customImages": [
  //   "https://gimg2.baidu.com/image_search/src=http%3A%2F%2Fwww.linuxidc.com%2Fupload%2F2019_03%2F19030220298109.png&refer=http%3A%2F%2Fwww.linuxidc.com&app=2002&size=f9999,10000&q=a80&n=0&g=0n&fmt=jpeg?sec=1612864833&t=d2a1c9707ba8fe0e1c96f1b544478ba3"
  // ],
  /* background end */
  // quick open lazygit
  "quickOpenLazygit.useITerm": true,
}